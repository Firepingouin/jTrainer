<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>JTrainer</servlet-name>
		<servlet-class>jtrain.JTrainerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JTrainer</servlet-name>
		<url-pattern>/jtrainer</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>JLogin</servlet-name>
		<servlet-class>jtrain.JLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JLogin</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>JSearch</servlet-name>
		<servlet-class>jtrain.JSearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSearch</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>JTrain</servlet-name>
		<servlet-class>jtrain.JTrainServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JTrain</servlet-name>
		<url-pattern>/train</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>jtrainer</welcome-file>
	</welcome-file-list>
</web-app>
